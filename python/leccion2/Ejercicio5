#Ejercicio 5

calificacion = None
nota = float(input("Digite la nota: "))
if 0 <= nota < 6:
    calificacion = "F"
elif 6 <= nota < 7:
    calificacion = "D"
elif 7 <= nota < 8:
    calificacion = "C"
elif 8 <= nota < 9:
    calificacion = "B"
elif 9 <= nota <= 10:
    calificacion = "A"
else:
    calificacion = "Ingreso erroneo"
print(f"La nota: {nota} corresponde a una calificacion de: {calificacion}")