# Clase 01/06

estacion = None
mes = int(input("Digite el valor para el mes: "))
if 1 <= mes <= 3:
    estacion = "verano"
elif 4 <= mes <= 6:
    estacion = "otoÃ±o"
elif 7 <= mes <= 9:
    estacion = "invierno"
elif 10 <= mes <= 12:
    estacion = "primavera"
else:
    estacion = "Ingreso erroneo"
print(f"El mes {mes} pertenece a la estacion: {estacion}")