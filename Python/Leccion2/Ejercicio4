# Clase 01/06

etapas = None
edad = int(input("Digite su edad: "))
if 0 <= edad <= 10:
    etapas = "La infancia es increible y bella"
elif 11 <= edad <= 19:
    etapas = "Tienes muchos cambios, mucho que estudiar"
elif 20 <= edad <= 29:
    etapas = "Amor y comienzas el trabajo"
elif 30 <= edad <= 40:
    etapas = "Familia, estudio y especializaciones para plenear un futuro"
elif 41 <= edad <= 50:
    etapas = "Viajes por el mundo"
elif 51 <= edad <= 60:
    etapas = "Disfrutar de los nietos"
elif 61 <= edad <= 100:
    etapas = "Descansaaaarrrrrrrrrrrrrrrrrrrrrr"
elif 100 <= edad <= 200:
    etapas = "Descansar en paz"
else:
    etapas = "Ingreso erroneo"
print(f"La edad: {edad} corresponde a la etapa: {etapas}")